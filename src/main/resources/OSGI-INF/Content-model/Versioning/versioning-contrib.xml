<?xml version="1.0" encoding="UTF-8"?>

<component name="org.athento.nuxeo.athento-nx-initial-version.versioning"
	version="1.0.0">
	<require>org.nuxeo.runtime.started</require>

	<extension target="org.nuxeo.ecm.core.versioning.VersioningService"
		point="versioningRules">

		<versioningRule typeName="File">
			<initialState major="1" minor="0" />
		</versioningRule>

		<versioningRule typeName="Picture">
			<initialState major="1" minor="0" />
		</versioningRule>

		<versioningRule typeName="Note">
			<initialState major="1" minor="0" />
		</versioningRule>

	</extension>

</component>